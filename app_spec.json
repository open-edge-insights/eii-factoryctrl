{
    "Module": {
       "Name": "FactoryControlApp",
       "Description": "Factory Control module manifest",
       "SchemaVersion": "0.2",
       "ManifestFile": "${idx:factoryctrlapp:$EIS_VERSION}",
       "RunAsUser": "$EIS_UID",
       "RunAsGroup": "$EIS_UID",
       "Resources": {
           "CPU": 100,
           "MemoryMB": 100
       },
       "ExecutionEnv": {
           "AppName": "FactoryControlApp",
           "PROFILING_MODE": "false",
           "ETCD_ENDPOINT": "${datastore.endpoint}",
           "CONFIGMGR_CACERT": "${databucket.cacert}",
           "CONFIGMGR_CERT": "${databucket.cert}",
           "CONFIGMGR_KEY": "${databucket.key}",
           "DEV_MODE": "$DEV_MODE",
           "ETCD_PREFIX": "$ETCD_PREFIX",
           "CertType": "zmq",
           "ZMQ_RECV_HWM": "50",
           "SubTopics": "VideoAnalytics/camera1_stream_results",
           "camera1_stream_results_cfg": "zmq_tcp,${ep.va-input.remoteaddress}:${ep.va-input.remoteport}"
       },
       "Endpoints": [
           {
               "Name": "va-input",
               "Endtype": "client",
               "DataType": "messages",
               "Link": "va-output",
               "Port": "29170"
           }
       ] 	    
    }
}
